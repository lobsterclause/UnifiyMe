{
  "name": "unifiyme",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "bin": {
    "unifi-mcp-server": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc && chmod +x dist/index.js",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "restricted-yt": "tsx src/restricted-yt-blocker.ts"
  },
  "keywords": [
    "unifi",
    "mcp",
    "mcp-server",
    "network-monitoring",
    "iot",
    "automation"
  ],
  "author": "Gabriel Phoenix",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lobsterclause/UnifiyMe.git"
  },
  "bugs": {
    "url": "https://github.com/lobsterclause/UnifiyMe/issues"
  },
  "homepage": "https://github.com/lobsterclause/UnifiyMe#readme",
  "files": [
    "dist/index.js",
    "dist/unifi/",
    "dist/firewall-manager.js",
    "dist/iot-vlan-manager.js",
    "dist/qos-manager.js",
    "dist/youtube-manager.js",
    "dist/gateway.js",
    "dist/monitor.js",
    "dist/exporter.js",
    "dist/discord-bot.js",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.3",
    "@types/ssh2": "^1.15.5",
    "async-retry": "^1.3.3",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "node-unifi": "^2.5.1",
    "prom-client": "^15.1.3",
    "ssh2": "^1.17.0"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.9",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.10",
    "@types/retry": "^0.12.5",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "prettier": "^3.8.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
